<?xml version="1.0" encoding="utf-8" ?>
<redirector ignoreRegex=".*(\.css|\.txt|\.js|\.gif|\.jpg|\.png)">
	<urlGroups>
		<add regex="/users/(\d+)$">
			<urls>
				<add regex="/users/(\d+)$" replacement="/users/$1/" />
			</urls>
		</add>
		<!--
________SEE MORE WORKING at http://www.codeproject.com/KB/aspnet/redirector.aspx_____________________________________
